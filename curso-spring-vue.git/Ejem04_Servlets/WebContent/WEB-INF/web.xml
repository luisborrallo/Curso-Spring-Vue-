	</welcome-file-list>
	<servlet>
		<servlet-name>HolaS</servlet-name>
		<servlet-class>com.grupoica.servlets.HolaServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HolaS</servlet-name>
		<url-pattern>/hola.do</url-pattern>
	</servlet-mapping>
</web-app>

